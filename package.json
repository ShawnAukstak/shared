{
  "name": "pydt-shared",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "npm run lint && ng build --prod pydt-shared-lib",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "swagger-codegen": "bash -c \"mkdir -p .tmp && cp ../api/api/_gen/swagger/swagger.json .tmp/swagger.json && MSYS_NO_PATHCONV=1 docker run --rm -v ${PWD}:/local swaggerapi/swagger-codegen-cli:2.4.9 generate -i /local/.tmp/swagger.json -l typescript-angular --additional-properties ngVersion=8 -o /local/projects/pydt-shared-lib/src/lib/_gen/swagger/api -DmodelPropertyNaming=original\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.12",
    "@angular/common": "^8.2.12",
    "@angular/compiler": "^8.2.12",
    "@angular/core": "^8.2.12",
    "@angular/forms": "^8.2.12",
    "@angular/platform-browser": "^8.2.12",
    "@angular/platform-browser-dynamic": "^8.2.12",
    "@angular/router": "^8.2.12",
    "core-js": "^3.3.4",
    "ngx-bootstrap": "3.3",
    "rxjs": "~6.5.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.14",
    "@angular-devkit/build-ng-packagr": "~0.803.14",
    "@angular/cli": "~8.3.14",
    "@angular/compiler-cli": "^8.2.12",
    "@angular/language-service": "^8.2.12",
    "@types/jasmine": "~3.4.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.11.7",
    "codelyzer": "~5.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.7.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.4.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.20.0",
    "typescript": "3.5"
  }
}
